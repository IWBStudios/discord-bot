{
  "name": "discord-bot",
  "version": "0.1.0",
  "description": "A Discord Bot",
  "scripts": {
    "build": "bun run tsc",
    "start": "bun run dist/index.js",
    "start:pm2": "pm2 start ecosystem.config.js --env production",
    "start:prod": "bun run build && bun run start:pm2",
    "dev": "bun run nodemon -L",
    "format": "prettier --write src"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/node-fetch": "^2.6.13",
    "nodemon": "^3.1.11",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "discord.js": "^14.24.2",
    "dotenv": "^17.2.3",
    "mysql2": "^3.15.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3"
  }
}
